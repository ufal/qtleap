<termEntry id="8899_165924"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A cookie that is deleted at the end of the browser session.</descrip></descripGrp><ntig><termGrp><term id="165924">session cookie</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1797771">saioaren cookie</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8913_56463"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A number that is not a whole number, such as ½ (vulgar fraction) or 0.5 (decimal fraction), formed by dividing one quantity into another.</descrip></descripGrp><ntig><termGrp><term id="56463">fraction</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1228183">zatiki</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8916_16592"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A file containing digitized audio, such as voice or music.</descrip></descripGrp><ntig><termGrp><term id="16592">audio file</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1094403">audio-fitxategi</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8916_1034316"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A file containing digitized audio, such as voice or music.</descrip></descripGrp><ntig><termGrp><term id="1034316">audio</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1094402">audio</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8922_34048"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A startup process that begins with turning on the computer's power. Typically, a cold boot involves some basic hardware checking by the system, after which the operating system is loaded from disk into memory.</descrip></descripGrp><ntig><termGrp><term id="34048">cold boot</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="439382">berrabiarazte hotz</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8924_112244"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The command interpreter that is used to pass commands to the operating system.</descrip></descripGrp><ntig><termGrp><term id="112244">shell</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="439897">shell</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8929_81571"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The device on which images generated by the computer's video adapter are displayed. The term monitor usually refers to a video display and its housing. The monitor is attached to the video adapter by a cable.</descrip></descripGrp><ntig><termGrp><term id="81571">monitor</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="439736">monitore</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8931_84940"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A group of one or more COM objects in a process. A COM object lives in exactly one apartment, in the sense that its methods can legally be directly called only by a thread that belongs to that apartment. Any other thread that wants to call the object must go through a proxy.</descrip></descripGrp><ntig><termGrp><term id="84940">apartment</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1990026">apartamentu</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8937_61873"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A list of the user's actions within a program, such as commands entered in an operating system shell, menus passed through using Gopher, or links followed using a Web browser.</descrip></descripGrp><ntig><termGrp><term id="61873">history</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="2085215">historia</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8945_117177"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An index entry that falls under a more general heading. For example, the index entry "planets" could have the subentries "Mars" and "Venus."</descrip></descripGrp><ntig><termGrp><term id="117177">subentry</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="439934">azpisarrera</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8949_140209"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">Related data points that are plotted in a chart. Each data series in a chart has a unique color or pattern and is represented in the chart legend. You can plot one or more data series in a chart. Pie charts have only one data series.</descrip></descripGrp><ntig><termGrp><term id="140209">data series</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="1227657">datu serie</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="8961_318125"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">Colors that are directly next to each other on the color wheel. Each color has two analogous colors (one on each side of it). </descrip></descripGrp><ntig><termGrp><term id="318125">analogous colors</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="440840">kolore analogoak</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="9001_396929"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A disk directory in which an SMTP service can deposit incoming mail messages.</descrip></descripGrp><ntig><termGrp><term id="396929">drop directory</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="427635">mezuak uzteko direktorioak</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="9010_347945"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A group in the data source that can occur more than once. Controls such as repeating sections and repeating tables are bound to repeating groups.</descrip></descripGrp><ntig><termGrp><term id="347945">repeating group</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet><langSet xml:lang="eu-es"><ntig><termGrp><term id="441056">errepikapen-talde</term><termNote type="partOfSpeech">Noun</termNote></termGrp></ntig></langSet></termEntry><termEntry id="9025_253693"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A data type that stores a signed, exact numeric value described as the number of digits appearing before and after the decimal point, with a maximum of 29 total digits. All possible digits cannot be represented if you are using the maximum number of digits.</descrip></descripGrp><ntig><termGrp><term id="253693">Decimal</term><ter